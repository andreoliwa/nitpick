{
  "name": "flake8-nitpick",
  "version": "0.10.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreoliwa/flake8-nitpick.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepare": "bumpversion --allow-dirty --no-commit --no-tag --new-version ${nextRelease.version} && poetry build",
          "publish": "twine upload --username andreoliwa --password $TWINE_PASSWORD -r testpypi dist/* && twine upload --username andreoliwa --password $TWINE_TEST_PASSWORD dist/*"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
